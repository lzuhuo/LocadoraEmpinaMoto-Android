<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.locadoraempinamoto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LocadoraEmpinaMoto">
        <activity
            android:name=".ui.moto.MotoItemActivity"
            android:label="@string/title_activity_moto_item"
            android:theme="@style/Theme.LocadoraEmpinaMoto.NoActionBar"></activity>
        <activity
            android:name=".ui.moto.MotoCadastroActivity"
            android:label="@string/title_activity_moto_cadastro"
            android:theme="@style/Theme.LocadoraEmpinaMoto.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.LocadoraEmpinaMoto.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>